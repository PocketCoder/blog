<%- include('partials/header.ejs') %>
    <div id="blog">
      <% if (posts !== []) { %>
        <% for (post of posts) { %>
          <a  class="post" href="/posts/<%= post.slice(0, -3) %>">
              &gt; <%= data[post].category + ': ' + data[post].title.toUpperCase() %>
          </a>
        <% } %>
      <% } else { %>
        <p>No posts yet!</p>
      <% } %>
    </div>
<%- include('partials/footer.ejs') %>